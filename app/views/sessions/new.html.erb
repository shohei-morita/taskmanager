<div class="wrapper">
<h1>Log in</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
<div class="form-group">
  <%= f.label :email %>
  <%= f.email_field :email, class: "form-control", id: "session-email" %>
</div>
<div class="form-group">
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control", id: "session-pw" %>
</div>
<div class="form-group">
  <%= f.submit "Log in", class: "btn btn-primary", id: "session-login" %>
</div>
<% end %>
</div>
